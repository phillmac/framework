{
    "name": "userfrosting/uniformresourcelocator",
    "type": "library",
    "description": "Uniform Resource Locator module for UserFrosting",
    "keywords": ["userfrosting", "Uniform Resource Locator", "URI", "StreamWrapper"],
    "homepage": "https://github.com/userfrosting/UniformResourceLocator",
    "license" : "MIT",
    "authors" : [
        {
            "name": "Louis Charette",
            "homepage": "https://bbqsoftwares.com"
        },
        {
            "name": "Alexander Weissman",
            "homepage": "https://alexanderweissman.com"
        }
    ],
    "require": {
        "php": ">=7.1",
        "rockettheme/toolbox": "~1.4.7",
        "illuminate/support": "^5.8",
        "illuminate/filesystem": "^5.8"
    },
	"require-dev": {
        "friendsofphp/php-cs-fixer": "^2.13",
        "phpunit/phpunit": "^7.5 | ^8.5",
        "victorjonsson/markdowndocs": "^1.3"
    },
    "autoload": {
