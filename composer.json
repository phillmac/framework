{
    "name": "userfrosting/session",
    "type": "library",
    "description": "Uniform interface for storing sessions in a variety of mediums, based on illuminate/session",
    "keywords": ["userfrosting", "session"],
    "homepage": "https://github.com/userfrosting/session",
    "license" : "MIT",
    "authors" : [
        {
            "name": "Alexander Weissman",
            "homepage": "https://alexanderweissman.com"
        }
    ],
    "require": {
        "illuminate/session": "^5.8",
        "illuminate/support": "^5.8",
        "php": ">=7.1"
    },
    "require-dev": {
        "friendsofphp/php-cs-fixer": "^2.13",
        "phpunit/phpunit": "^7.5"
    },
    "autoload": {
        "psr-4": {
            "UserFrosting\\Session\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "UserFrosting\\Session\\Tests\\": "tests"
        }
    }
}
